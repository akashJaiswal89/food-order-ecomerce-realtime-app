<section class='status'>
    <div class=" mx-auto">
        <div class="w-2/3 mx-auto status-box">
            <div class="flex item-center justify-between mb-12">
                <h1 class="text-xl font-bold singleFont">
                    Track dilevery status
                </h1>
                <h6 class="bg-white py-1 rounded-full px-4 text-green-600 text-xs call">
                    Call-8922988589
                </h6>
            </div>
            <input id="headden" value='<%-JSON.stringify(order)%>' type="hidden">
            <ul>
                <li class="status_line text-xl pb-8 " data-status='order_placed'><span>Order Placed</span></li>
                <li class="status_line text-xl pb-8" data-status='confirmed'><span>Order Confimation</span></li>
                <li class="status_line text-xl pb-8 " data-status='prepared'><span>Order prepation</span></li>
                <li class="status_line text-xl pb-8" data-status='delivered'><span>Order out for delevery</span></li>
                <li class="status_line text-xl " data-status='completed'><span>Order completed</span></li>
            </ul>
        </div>
    </div>
</section>